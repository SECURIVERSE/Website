var Globals = {
	getDefaultStyles: function(isStore) {
		var styles = {
			button_color: '#e60094',
			button_border: {
				color: '#cccccc',
				style: 'none',
				weight: 1,
				css: { border: '1px none #cccccc' },
				cssRaw: 'border: 1px none #cccccc'
			},
			logo: 'paytrail_white_line.svg',
			font_family: 'Arial',
