			get: function(data) { return data.content.price; },
			validate: function(value) {
				if (/^[\d.,]+$/.test(value)) {
					return '';
				} else {
					return (__('Incorrect format'));
				}
			},
			set: function(value, data) {
				data.content.price = value;
			}
		},
