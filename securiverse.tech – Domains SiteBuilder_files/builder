ton\">{{\"Pay\" | tr}}<\/button>\r\n\t\t\t<\/div>\r\n\t\t<\/div>\r\n\t<\/div>\r\n<\/div>\r\n<script type=\"text\/javascript\">\r\n\t$('#{{id}}_braintree_modal').appendTo('body');\r\n<\/script>\r\n{{\/isPublished}}";
