				button_color: '#ffffff',
				button_border: {
					color: '#cccccc',
					style: 'solid',
					weight: 1,
					css: {border: '1px solid #cccccc'},
					cssRaw: 'border: 1px solid #cccccc'
				},
				logo: 'aamarpay_color.svg',
				font_family: 'Arial',
				font_size: 12,
