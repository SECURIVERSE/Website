		{id: 'description2', type: 'StripeDescription2',
			get: function(data) { return data.content.description2; },
			set: function(value, data) {
				data.content.description2 = value;
			}
		},
	],Globals.getLogosList(), 'stripe'),
	propertyDialog: {
