								{id: 'THB', name: 'THB'},
								{id: 'TOP', name: 'TOP'},
								{id: 'TRY', name: 'TRY'},
								{id: 'TTD', name: 'TTD'},
								{id: 'TWD', name: 'TWD'},
								{id: 'UAH', name: 'UAH'},
