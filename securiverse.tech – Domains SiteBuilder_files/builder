var Globals = {
	getDefaultStyles: function(isStore) {
		var styles = {
			button_color: '#fff',
			button_border: {
				color: '#000',
				style: 'solid',
				weight: 1,
				css: { border: '1px solid #000' },
				cssRaw: 'border: 1px solid #000'
			},
			logo: 'mollie_color.svg',
